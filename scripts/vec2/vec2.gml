	isvec2 = true;
}
function vec22(x_=0, y_=0, x2_=0, y2_=0) constructor {
	x = x_;
	y = y_;
	x2 = x2_;
	y2 = y2_;
	isvec22 = true;
}
function vec3(x_=0, y_=0, z_=0) constructor {
